<div class="datepicker-wrap J_datepicker-wrap">
   <ul class="candidate-list">
       <li class="candidate-item" date-format="YYYYMMDD">{{YYYYMMDD}}</li>
       <li class="candidate-item" date-format="YYYY-MM-DD">{{YYYY-MM-DD}}</li>
       <li class="candidate-item" date-format="YYYY/MM/DD">{{YYYY/MM/DD}}</li>
       <li class="candidate-item" date-format="YYYY年MM月DD日">{{YYYY年MM月DD日}}</li>
       <li class="candidate-item" date-format="YYYY-MM-DD hh:mm">{{YYYY-MM-DD hh:mm}}</li>
   </ul>
    <button class="cancel">取消</button>
</div>